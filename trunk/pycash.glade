    <property name="title" translatable="yes">main.c</property>
    <property name="window_position">GTK_WIN_POS_CENTER</property>
    <property name="default_width">800</property>
    <property name="default_height">600</property>
    <child>
      <widget class="GtkScrolledWindow" id="scrolledwindow4">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="shadow_type">GTK_SHADOW_IN</property>
        <child>
          <widget class="GtkTextView" id="textview1">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="text" translatable="yes">#include &lt;stdio.h&gt;
#include &lt;stdlib.h&gt;
#include &lt;string.h&gt;

/*not support chinese words!*/
char* Base64_alphabet = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=";
char* Base64_encode(char data[], int length)
{
	int i, index, val;
	int quad, trip;  /* bool type: 0 or 1 */
	char* out;
	out = malloc( (((length+2)/3)*4) * sizeof(char) );
	for (i=0, index=0; i&lt;length; i+=3, index+=4) {
		quad = 0;
		val = (0xFF &amp; (int) data[i]);
		val &lt;&lt;= 8;
		if ((i+1) &lt; length) {
			val |= (0xFF &amp; (int) data[i+1]);
			trip = 1;
		}
		val &lt;&lt;= 8;
		if ((i+2) &lt; length) {
			val |= (0xFF &amp; (int) data[i+2]);
			quad = 1;
		}
		out[index+3] = Base64_alphabet[(quad ? (val &amp; 0x3F): 64)];
		val &gt;&gt;= 6;
		out[index+2] = Base64_alphabet[(trip ? (val &amp; 0x3F): 64)];
		val &gt;&gt;= 6;
		out[index+1] = Base64_alphabet[val &amp; 0x3F];
		val &gt;&gt;= 6;
		out[index+0] = Base64_alphabet[val &amp; 0x3F];

	}
	out[index] = '\0';
	return out;
}
void Base64_init_codes(char codes[])
{
	int i;
	for (i = 0; i &lt; 256; i++) codes[i] = -1;
	for (i = 'A'; i &lt;= 'Z'; i++) codes[i] = i - 'A';
	for (i = 'a'; i &lt;= 'z'; i++) codes[i] = 26 + i - 'a';
	for (i = '0'; i &lt;= '9'; i++) codes[i] = 52 + i - '0';
	codes['+'] = 62;
	codes['/'] = 63;
}
char* Base64_decode(char data[],int length)
{
	int value, ix;
	int shift = 0;   /* # of excess bits stored in accum */
	int accum = 0;   /* excess bits */
	int index = 0;
	int len;
	char codes[256];
	char* out;
	len = ((length + 3) / 4) * 3;
	if (length&gt;0 &amp;&amp; data[length-1] == '=') --len;
	if (length&gt;1 &amp;&amp; data[length-2] == '=') --len;
	out = (char*)malloc(len * sizeof(char));
	Base64_init_codes(codes);
	for (ix=0; ix&lt;length; ix++) {
		value = codes[ data[ix] &amp; 0xFF ]; /* ignore high byte of char */
		if ( value &gt;= 0 ) {     /* skip over non-code */
			accum &lt;&lt;= 6;            /* bits shift up by 6 each time thru */
			shift += 6;             /* loop, with new bits being put in */
			accum |= value;         /* at the bottom. */
			if ( shift &gt;= 8 ) {     /* whenever there are 8 or more shiftedin, */
				shift -= 8;         /* write them out (from the top, leaving any */
				out[index++] =      /* excess at the bottom for next iteration. */
				        ((accum &gt;&gt; shift) &amp; 0xff);
			}
		}
	}
	out[index] = '\0';
	if (index != len) printf("miscalculated data length!\n");
	return out;
}
int main()
{
	char* data1 = "b";
	char* data2 = "1234567890";
	char* out1;
	char* out2;
	out1 = Base64_encode(data1,strlen(data1));
	out2 = Base64_encode(data2,strlen(data2));
	printf("[%s]\n[%s]\n",out1,out2);
	data1 = Base64_decode(out1, strlen(out1));
	data2 = Base64_decode(out2, strlen(out2));
	printf("[%s]\n[%s]\n",data1,data2);
	free(out1);
	free(out2);

}
